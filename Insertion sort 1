#include <stdio.h>
int main()
{
    int a[100],n,key,i,j;
    printf("How many elements you want ? ");
    scanf("%d",&n);
    printf("Enter data :\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Data before sorting : \n");
     for(int i=0;i<n;i++)
    printf("%d ",a[i]);
    for(int i=1;i<n;i++)
    {
        key=a[i];
        j=i-1;
        while(j>=0 && a[j]>key)
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=key;
    }
    printf("\nData After sorting : \n");
    for(int i=0;i<n;i++)
    printf("%d ",a[i]);
}


